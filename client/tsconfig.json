{
    "extends": "../tsconfig.json",
    "compilerOptions": {
      "jsx": "react-jsx",
      "baseUrl": ".",
      "types": ["vitest/globals"],
      "paths": {
        "@/*": ["src/*"],
        "@api/*": ["src/api/*"],
        "@components/*": ["src/components/*"],
        "@hooks/*": ["src/hooks/*"]
